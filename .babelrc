{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "ie >= 11"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "env": {
        "test": {
            "presets": [
                "env",
                "stage-2"
            ],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "root": [
                            "./src"
                        ],
                        "alias": {
                            "@": "./src"
                        }
                    }
                ]
            ]
        }
    },
    "plugins": [
        "transform-vue-jsx",
        "transform-runtime",
        ["transform-imports", {
            "vuetify": {
                "transform": "vuetify/lib/${member}",
                "preventFullImport": true
            }
        }]
    ]
}